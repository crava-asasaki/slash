{
    "ect":{
        "LIB_URL": "lib/",
        "JS_URL": "js/",
        "CSS_URL": "css/",
        "IMAGE_URL": "images/",
        "BASE_URL": "./"
    },
    "autoprefixer": {
        "browser": ["last 3 version", "ie >= 9", "Android 4.0"]
    },
    "sprite": {
        "extension": ".png",
        "imgExtension": ".png",
        "cssExtension": ".scss",
        "options": {
            "algorithm": "binary-tree",
            "padding": 5
        }
    }
}